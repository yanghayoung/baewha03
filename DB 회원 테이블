회원 테이블
create table member(
name nvarchar(30) NOT NULL,
email nvarchar(50) NOT NULL,
phoneN char(30) NOT NULL,
birth char(20) NOT NULL,
gen char(2),
bike nvarchar(50),
cc char(10),
earphone char(2),
)


